        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#ElectricalNetworkCosts"/>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#CostsForLand -->

    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#CostsForLand">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#IndividualCosts"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#CostsForSystemsRealization"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#GeneralExpense"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#InstallationCostsForSystemsRealization"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#ManufacturingCosts"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#NonManufacturingFixedCapitalInvestment"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#ServiceFacilityCosts"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#WorkingCapital"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CostsForLand cover the land price and the yard improvement costs.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#CostsForSystemsRealization -->

    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#CostsForSystemsRealization">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#AccumulatedCosts"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#addsUp"/>
                <owl:someValuesFrom rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#InstallationCostsForSystemsRealization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#addsUp"/>
                <owl:someValuesFrom rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#PurchaseCostsForSystemsRealization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#addsUp"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#InstallationCostsForSystemsRealization"/>
                            <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#PurchaseCostsForSystemsRealization"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#addsUp"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#FixedCapitalInvestment"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#ManufacturingFixedCapitalInvestment"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#ProductionCosts"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#PurchaseCostsForSystemsRealization"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#ServiceFacilityCosts"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#TotalCapitalInvestment"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#TotalENCosts"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CostsForSystemsRealization are directly associated with the ProcessingSubsystem and the OperatingSubsystem.</rdfs:comment>
    </owl:Class>
    


