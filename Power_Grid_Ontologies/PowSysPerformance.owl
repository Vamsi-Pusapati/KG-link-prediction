    <!-- http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#AccumulatedCosts -->

    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#AccumulatedCosts">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#ElectricalNetworkCosts"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#addsUp"/>
                        <owl:someValuesFrom rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#ElectricalNetworkCosts"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#EquipmentCosts"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#IndividualCosts"/>
        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#InstrumentationCosts"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AccumulatedCosts summarize all partial costs that may occur in a ChemicalProcessSystem.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontopowsys/PowSysPerformance.owl#CapitalExpenditureCosts -->

